{"ast":null,"code":"import StoredResources from './StoredResources.vue';\nimport AddResource from './AddResource.vue';\nexport default {\n  name: 'TheHeader',\n  components: {\n    StoredResources,\n    AddResource\n  },\n  data() {\n    return {\n      selectedTab: 'add-resource',\n      storedResources: [{\n        id: 1,\n        title: 'Officiel Doc',\n        description: 'Vue.js - The Progressive JavaScript Framework',\n        link: 'https://vuejs.org/',\n        icon: 'school'\n      }, {\n        id: 2,\n        title: 'Google',\n        description: 'Google',\n        link: 'https://www.google.com/',\n        icon: 'book'\n      }]\n    };\n  },\n  provide() {\n    return {\n      resources: this.storedResources\n    };\n  },\n  computed: {\n    storedResourcesButtonMode() {\n      return this.selectedTab === 'stored-resources' ? 'raised' : 'flat';\n    },\n    addResourceButtonMode() {\n      return this.selectedTab === 'add-resource' ? 'raised' : 'flat';\n    }\n  },\n  methods: {\n    setSelectedTab(tab) {\n      this.selectedTab = tab;\n    },\n    addResource(title, description, link) {\n      const id = this.storedResources.length + 1;\n      this.storedResources.unsg({\n        id,\n        title,\n        description,\n        link,\n        icon: 'book'\n      });\n      this.selectedTab = 'stored-resources';\n    }\n  }\n};","map":{"version":3,"names":["StoredResources","AddResource","name","components","data","selectedTab","storedResources","id","title","description","link","icon","provide","resources","computed","storedResourcesButtonMode","addResourceButtonMode","methods","setSelectedTab","tab","addResource","length","unsg"],"sources":["/Users/manulabricole/Documents/Bricolerie/vue_js/section-10-projet/src/components/learning-resources/TheResource.vue"],"sourcesContent":["<template>\n  <base-card>\n    <div class=\"button-container\">\n      <base-button\n        @click=\"setSelectedTab('stored-resources')\"\n        type=\"button\"\n        :